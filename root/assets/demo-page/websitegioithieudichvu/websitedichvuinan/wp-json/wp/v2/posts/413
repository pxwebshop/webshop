 th\u01b0\u1eddng d\u00f9ng l\u00e0 in h\u00f3a \u0111\u01a1n b\u00e1n h\u00e0ng, in h\u00f3a \u0111\u01a1n b\u00e1n l\u1ebb hay h\u00f3a \u0111\u01a1n tr\u1ef1c ti\u1ebfp. C\u00e1c lo\u1ea1i h\u00f3a [&#8230;]\n","protected":false},"author":1,"featured_media":414,"comment_status":"open","ping_status":"open","sticky":false,"template":"","format":"standard","meta":[],"categories":[20],"tags":[],"_links":{"self":[{"href":"http:\/\/print2.websiteviet.vn\/wp-json\/wp\/v2\/posts\/413"}],"collection":[{"href":"http:\/\/print2.websiteviet.vn\/wp-json\/wp\/v2\/posts"}],"about":[{"href":"http:\/\/print2.websiteviet.vn\/wp-json\/wp\/v2\/types\/post"}],"author":[{"embeddable":true,"href":"http:\/\/print2.websiteviet.vn\/wp-json\/wp\/v2\/users\/1"}],"replies":[{"embeddable":true,"href":"http:\/\/print2.websiteviet.vn\/wp-json\/wp\/v2\/comments?post=413"}],"version-history":[{"count":1,"href":"http:\/\/print2.websiteviet.vn\/wp-json\/wp\/v2\/posts\/413\/revisions"}],"predecessor-version":[{"id":415,"href":"http:\/\/print2.websiteviet.vn\/wp-json\/wp\/v2\/posts\/413\/revisions\/415"}],"wp:featuredmedia":[{"embeddable":true,"href":"http:\/\/print2.websiteviet.vn\/wp-json\/wp\/v2\/media\/414"}],"wp:attachment":[{"href":"http:\/\/print2.websiteviet.vn\/wp-json\/wp\/v2\/media?parent=413"}],"wp:term":[{"taxonomy":"category","embeddable":true,"href":"http:\/\/print2.websiteviet.vn\/wp-json\/wp\/v2\/categories?post=413"},{"taxonomy":"post_tag","embeddable":true,"href":"http:\/\/print2.websiteviet.vn\/wp-json\/wp\/v2\/tags?post=413"}],"curies":[{"name":"wp","href":"https:\/\/api.w.org\/{rel}","templated":true}]}}